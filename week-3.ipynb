    "#P=2*M*np.exp(-2*N*e**2)\n",
    "import numpy as np\n",
    "Pmax=0.03\n",
    "e=0.05\n",
    "def calc_nmin(M):\n",
    "    Nmin=np.log(2*M / Pmax) / (2 * e**2)\n",
    "    print(\"M={:3d}  Nmin={:.0f}\".format(M, Nmin))\n",
    "calc_nmin(M=1)\n",
    "calc_nmin(M=10)\n",
    "calc_nmin(M=100)"
   ]
  }
